{
  "name": "ts-dingtalk-robot",
  "version": "1.0.0",
  "author": "CaoMeiYouRen",
  "description": "Typescript 版钉钉群机器人包",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src --fix --ext .ts,.js",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "rm": "rimraf node_modules",
    "debug": "cross-env DEBUG=ts-dingtalk-robot* ts-node-dev src/index.ts",
    "test": "cross-env DEBUG=ts-dingtalk-robot* ts-node test/index.test.ts"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "colors": "^1.4.0",
    "crypto-js": "^4.0.0",
    "debug": "^4.1.1",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.47",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.0.9",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
